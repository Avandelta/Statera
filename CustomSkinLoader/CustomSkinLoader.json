{
  "version": "14.12",
  "loadlist": [
    {
      "name": "LocalSkin",
      "type": "Legacy",
      "checkPNG": true,
      "skin": "Local/Skin/{USERNAME}.png",
      "cape": "Local/Cape/{USERNAME}.png",
      "elytra": "Local/Elytra/{USERNAME}.png",
      "model": "auto"
    }
  ],
  "enableDynamicSkull": true,
  "enableTransparentSkin": true,
  "ignoreHttpsCertificate": true,
  "forceLoadAllTextures": true,
  "enableCape": true,
  "threadPoolSize": 1,
  "cacheExpiry": 50,
  "enableUpdateSkull": true,
  "enableLocalProfileCache": false,
  "enableCacheAutoClean": true
}